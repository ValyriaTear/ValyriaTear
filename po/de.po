#: ../dat/battles/tutorial_battle_dialogs.lua:75
#: ../dat/battles/tutorial_battle_dialogs.lua:102 ../dat/config/quests.lua:82
#: ../dat/maps/layna_forest/layna_forest_cave1_2_script.lua:201
#: ../dat/maps/layna_forest/layna_forest_wolf_cave_script.lua:210
